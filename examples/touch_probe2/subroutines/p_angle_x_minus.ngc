o<p_angle_x_minus> sub
	G20
	#1004 = .5				; store fixed length side B
	#1010 = #<_hal[flexhal.probe_SEARCH_FEED]>
	#1011 = #<_hal[flexhal.probe_FINE_FEED]>
   	F #1010
	G91 G38.2 X -.5			; locate start point
	G91 G1 X .1				; back out a fixed dist
	G91 G1 Y #1004			; move to end of side B
	G91 G1 X -.1			; move back in back out a fixed dist
	G91 G38.3 Z-.00001		; trigger the point for side A
	#1002 = #5061			; store first Y point
	G91 G38.2 X -.5			; locate 2nd point for side A
	#1003 = #5061			; store 2nd Y point
	G91 G1 X .1				; back out a fixed dist
	#1001 = [#1002 - #1003]	; length of side A
	#1005 = ATAN[#1001]/[#1004]
	(debug, #1005 deg)
o<p_angle_x_minus> endsub
M2
