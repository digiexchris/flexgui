o<p_angle_y_plus_m> sub
	G21
	#1010 = #<_hal[flexhal.probe_SEARCH_FEED]>
	#1011 = #<_hal[flexhal.probe_FINE_FEED]>
  	F #1010
	#1001 = 2.54				; store side a length
	G91 G38.2 Y 12.7			; locate start point
	G91 G1 Y -2.54				; move to side a
	G91 G38.3 Z-.000254			; trigger the first point read
	#1002 = #5061				; store first Y point
	G91 G38.2 F#1011 X 50.8		; slow down for 2nd point
	#1003 = #5061				; store 2nd Y point
	G91 G1 Y -2.54				; retract a bit
	#1004 = [#1002 - #1003]
	#1005 = [90+ATAN[#1004]/[#1001]]
	(debug, #1005 deg)
	o<p_angle_y_plus_m> endsub
